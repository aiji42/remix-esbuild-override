    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.3.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@remix-run/dev": "^1.3.3",
    "@remix-run/eslint-config": "^1.3.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "eslint": "^8.11.0",
    "miniflare": "^2.1.0",
    "npm-run-all": "^4.1.5",
