{
  "name": "remix-template-cloudflare-workers",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "main": "build/index.js",
  "scripts": {
    "build": "remix build",
    "deploy": "npm run build && wrangler publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev": "remix build && run-p dev:*",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "generate": "cross-env PRISMA_CLIENT_ENGINE_TYPE=dataproxy prisma generate",
    "postinstall": "patch-package && remix-esbuild-override"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "@remix-run/cloudflare": "^1.4.0",
    "@remix-run/cloudflare-workers": "^1.4.0",
    "@remix-run/react": "^1.4.0",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@remix-run/dev": "^1.4.0",
    "@remix-run/eslint-config": "^1.4.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "esbuild-plugin-alias": "^0.2.1",
    "eslint": "^8.11.0",
    "miniflare": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "prisma": "^3.12.0",
    "remix-esbuild-override": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
